<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HenCoder1">
        <!--字体颜色-->
        <attr name="textColor" format="color"/>
        <!--字体大小-->
        <attr name="textSize" format="dimension"/>
        <!--边角颜色-->
        <attr name="edgeColor" format="color"/>
        <!--区域颜色-->
        <attr name="areaColor" format="color"/>
        <!--点的颜色-->
        <attr name="pointColor" format="color"/>
        <!--里面有多少圈-->
        <attr name="loopCount" format="integer"/>
        <!--有几个角-->
        <attr name="edgeCount" format="integer"/>
    </declare-styleable>
</resources>